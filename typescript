    CONSTRAINT "FK_Pets_PetOwners_petOwnerid" FOREIGN KEY ("petOwnerid") REFERENCES "PetOwners" (id) ON DELETE CASCADE
);


CREATE INDEX "IX_Pets_petOwnerid" ON "Pets" ("petOwnerid");


