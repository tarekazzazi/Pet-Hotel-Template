[39;49m[39;49m[37m[39;49m[97mCREATE TABLE "PetOwners" (
    id integer GENERATED BY DEFAULT AS IDENTITY,
    name text NULL,
    "emailAddress" text NULL,
    "petCount" integer NOT NULL,
    CONSTRAINT "PK_PetOwners" PRIMARY KEY (id)
);


CREATE TABLE "Pets" (
    id integer GENERATED BY DEFAULT AS IDENTITY,
    name text NOT NULL,
    color integer NOT NULL,
    breed integer NOT NULL,
    "checkedInAt" timestamp without time zone NULL,
    "petOwnerid" integer NOT NULL,
    CONSTRAINT "PK_Pets" PRIMARY KEY (id),
    CONSTRAINT "FK_Pets_PetOwners_petOwnerid" FOREIGN KEY ("petOwnerid") REFERENCES "PetOwners" (id) ON DELETE CASCADE
);


CREATE INDEX "IX_Pets_petOwnerid" ON "Pets" ("petOwnerid");


